{
  "name": "@gestell/sdk",
  "version": "1.1.15",
  "description": "The Official Gestell Node.js SDK",
  "private": false,
  "repository": "https://github.com/Gestell-AI/node-sdk.git",
  "author": "Chris Cates <hello@chriscates.ca>",
  "license": "MIT",
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b ./dist/tsconfig.esm.json ./dist/tsconfig.cjs.json ./dist/tsconfig.types.json && tsc-alias -p ./dist/tsconfig.esm.json && tsc-alias -p ./dist/tsconfig.cjs.json",
    "test": "bun test --timeout 120000 --bail --coverage --coverage-reporter lcov",
    "test:submit": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint ."
  },
  "dependencies": {
    "dotenv": "16.4.7",
    "mime-types": "2.1.35",
    "node-fetch": "2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@types/bun": "1.1.14",
    "@types/mime-types": "2.1.4",
    "@types/node-fetch": "2",
    "coveralls": "3.1.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "tsc-alias": "1.8.10",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.0"
  }
}